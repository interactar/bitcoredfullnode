{
  "network": "livenet",
  "port": 3001,
  "services": [
    "bitcoind",
    "web",
    "insight-api"
  ],
  "servicesConfig": {
    "bitcoind": {
      "spawn": {
        "datadir": "./data",
        "exec": "/usr/local/lib/node_modules/bitcore/node_modules/bitcore-node/bin/bitcoind"
      }
    },
    "insight-api": {
      "disableRateLimiter": true
    },
    "web": {
      "jsonRequestLimit": "200000000kb"
    },
    "address": {
    "maxInputsQueryLength": 500000,
    "maxOutputsQueryLength": 500000
    }
  }
}